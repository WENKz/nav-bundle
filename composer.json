{
    "name": "vincentchalamon/nav-bundle",
    "description": "Symfony bundle for Microsoft Dynamics Nav",
    "homepage": "https://github.com/vincentchalamon/nav-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["symfony","symfony-bundle","microsoft dynamic nav", "navision"],
    "readme": "README.md",
    "authors": [
        {
            "name": "Vincent Chalamon",
            "email": "vincentchalamon@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "vincentchalamon+support-navision@gmail.com"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vincentchalamon/ntlm-soap-client"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-dom": "*",
        "ext-soap": "*",
        "doctrine/annotations": "^1.0",
        "matejsvajger/ntlm-soap-client": "dev-master",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/property-access": "^4.4 || ^5.0",
        "symfony/property-info": "^4.4 || ^5.0",
        "symfony/serializer": "^4.4 || ^5.0",
        "symfony/stopwatch": "^4.4 || ^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "nyholm/symfony-bundle-test": "^1.4",
        "phpstan/phpstan": "^0.11.19",
        "phpunit/phpunit": "^8.0",
        "symfony/dotenv": "^4.4 || ^5.0",
        "symfony/twig-bundle": "^4.4 || ^5.0",
        "symfony/web-profiler-bundle": "^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "NavBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NavBundle\\Tests\\": "tests/",
            "NavBundle\\E2e\\": "features/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
