<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="nav.class_metadata.driver.annotation" class="NavBundle\ClassMetadata\Driver\AnnotationClassMetadataDriver" public="false" abstract="true">
            <argument type="service" id="annotations.reader"/>
        </service>

        <service id="nav.registry" class="NavBundle\Registry" public="true">
            <argument/> <!-- managers indexed by name -->
        </service>
        <service id="NavBundle\Registry" alias="nav.registry" public="true"/>
        <service id="NavBundle\RegistryInterface" alias="nav.registry" public="true"/>

        <service id="nav.abstract_manager" class="NavBundle\Manager\Manager" public="false" abstract="true">
            <argument/> <!-- driver -->
            <argument type="service" id="serializer"/>
            <argument type="service" id="config_cache_factory"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="tagged_locator" tag="nav.repository"/>
            <argument type="service" id="logger" on-invalid="null"/>
            <argument/> <!-- WSDL -->
            <argument/> <!-- authNtlm -->
            <argument>%kernel.cache_dir%/nav</argument>
        </service>

        <service id="nav.cache_warmer.manager" class="NavBundle\CacheWarmer\ManagerCacheWarmer" public="false">
            <argument type="service" id="service_container"/>
            <tag name="kernel.cache_warmer"/>
        </service>

        <service id="nav.type_factory" class="NavBundle\Type\TypeFactory" public="false">
            <argument type="tagged" tag="nav.type"/>
        </service>

        <service id="nav.type.array" class="NavBundle\Type\ArrayType" public="false">
            <tag name="nav.type"/>
        </service>

        <service id="nav.type.bool" class="NavBundle\Type\BoolType" public="false">
            <tag name="nav.type"/>
        </service>

        <service id="nav.type.date" class="NavBundle\Type\DateType" public="false">
            <tag name="nav.type"/>
        </service>

        <service id="nav.type.float" class="NavBundle\Type\FloatType" public="false">
            <tag name="nav.type"/>
        </service>

        <service id="nav.type.int" class="NavBundle\Type\IntType" public="false">
            <tag name="nav.type"/>
        </service>

        <service id="nav.type.string" class="NavBundle\Type\StringType" public="false">
            <tag name="nav.type"/>
        </service>

        <service id="nav.property_info.class_metadata_extractor" class="NavBundle\ClassMetadata\ClassMetadataExtractor" public="false">
            <argument type="service" id="nav.registry"/>
            <argument type="service" id="nav.type_factory"/>
            <tag name="property_info.type_extractor"/>
        </service>

        <service id="nav.decoder.read_multiple_result" class="NavBundle\Serializer\ReadMultipleResultDecoder" public="false">
            <tag name="serializer.encoder" priority="-1"/>
        </service>

        <service id="nav.decoder.read_result" class="NavBundle\Serializer\ReadResultDecoder" public="false">
            <tag name="serializer.encoder" priority="-1"/>
        </service>

        <service id="nav.normalizer.object" class="NavBundle\Serializer\ObjectNormalizer" public="false">
            <argument type="service" id="nav.registry"/>
            <tag name="serializer.normalizer" priority="-2"/>
        </service>

        <service id="nav.normalizer.collection" class="NavBundle\Serializer\CollectionNormalizer" public="false">
            <argument type="service" id="nav.registry"/>
            <tag name="serializer.normalizer" priority="-1"/>
        </service>

        <service id="nav.param_converter" class="NavBundle\Request\ParamConverter" public="false">
            <argument type="service" id="nav.registry"/>
            <tag name="request.param_converter" priority="-1"/>
        </service>
    </services>
</container>
